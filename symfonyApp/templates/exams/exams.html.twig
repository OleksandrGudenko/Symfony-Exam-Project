{% extends 'base.html.twig' %}

{% block stylesheets %}
    <link href="{{ asset('css/list.css') }}" rel="stylesheet" />
{% endblock %}

{% block body %}

    <div>
        <table>
            <thead>
            <tr>
                <th>Exam ID</th>
                <th>Course</th>
                <th>Course ID</th>
            </tr>
            </thead>
            <tbody>
            {% for exam in listData %}
                <tr>
                    <td>{{ exam.id }}</td>
                    <td>{{ exam.name }}</td>
                    <td>{{ exam.creator.firstname }}</td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    </div>

{% endblock %}